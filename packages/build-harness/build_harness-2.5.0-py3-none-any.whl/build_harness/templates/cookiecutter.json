{
  "author": "My Name",
  "author_email": "someone@somewhere.com",
  "build_harness_version": ">=2.1, <3",
  "coverage_threshold": 90,
  "creation_year": "1700",
  "default_branch": [
    "main",
    "master"
  ],
  "default_release_id": "0.0.0",
  "enable_venv": true,
  "line_length": 80,
  "packaging_provider": [
    "flit"
  ],
  "pipeline_artifact_expiry": "1 week",
  "pipeline_provider": [
    "gitlabci"
  ],
  "project_name": "my-project",
  "project_slug": "{{ cookiecutter.project_name.lower().replace(' ', '_').replace('-', '_') }}",
  "project_summary": "A project that does something.",
  "project_url": "https://gitlab.com/my_project/",
  "_venv_bin": ".venv/bin"
}
